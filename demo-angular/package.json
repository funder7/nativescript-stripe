{
  "description": "NativeScript Stripe - (demo) Angular Application",
  "license": "MIT",
  "readme": "README.md",
  "repository": "<fill-your-repository-here>",
  "nativescript": {
    "id": "org.nativescript.stripe.demoangular",
    "nsc-ios": {
      "version": "6.5.2"
    },
    "nsc-android": {
      "version": "7.0.0"
    }
  },
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@nativescript/angular": "10.0.0",
    "@nativescript/core": "~7.0.0",
    "@nativescript/theme": "^2.3.3",
    "nativescript-stripe": "file:../src",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~10.0.0",
    "@nativescript/webpack": "~3.0.0",
    "@ngtools/webpack": "~10.1.0",
    "lazy": "^1.0.11",
    "tslint": "~5.4.3",
    "typescript": "~3.9.0"
  },
  "scripts": {
    "tsc": "tsc",
    "clean": "npx rimraf hooks node_modules package-lock.json platforms && npm i",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "build.all": "npm run build.android && npm run build.ios",
    "build.android": "nsc build android",
    "build.ios": "nsc build ios",
    "run.android": "nsc run android",
    "run.ios": "nsc run ios",
    "emulator.android": "nsc run android --emulator",
    "emulator.ios": "nsc run ios --emulator",
    "android": "nsc run android --no-hmr",
    "ios": "nsc run ios --no-hmr",
    "mobile": "nsc run",
    "preview": "nsc preview",
    "postinstall": "npm run ngcc",
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'app/**/*.ts' --exclude '**/node_modules/**'"
  }
}
