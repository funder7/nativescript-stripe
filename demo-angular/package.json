{
  "name": "nativescript-stripe-demo-ng",
  "description": "NativeScript Stripe - (demo) Angular Application",
  "license": "MIT",
  "readme": "README.md",
  "repository": "<fill-your-repository-here>",
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "@nativescript/angular": "~10.1.0",
    "@nativescript/core": "~7.0.0",
    "@nativescript/theme": "~2.3.0",
    "reflect-metadata": "~0.1.12",
    "rxjs": "^6.6.0",
    "zone.js": "~0.11.1",
    "nativescript-stripe": "file:../src"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~10.1.0",
    "@nativescript/android": "7.0.0",
    "@nativescript/ios": "7.0.0",
    "@nativescript/types": "~7.0.0",
    "@nativescript/webpack": "~3.0.0",
    "@ngtools/webpack": "~10.1.0",
    "typescript": "~3.9.0"
  },
  "scripts": {
    "postinstall": "npm run ngcc",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "clean": "npx rimraf hooks node_modules package-lock.json platforms && npm i",
    "build.all": "npm run build.android && npm run build.ios",
    "build.android": "ns build android",
    "build.ios": "ns build ios",
    "run.android": "ns run android --no-hmr",
    "run.ios": "ns run ios --no-hmr",
    "emulator.android": "ns run android --emulator",
    "emulator.ios": "ns run ios --emulator",
    "mobile": "ns run",
    "preview": "ns preview",
    "build.plugin": "cd ../src && npm run build",
    "ci.tslint": "npm i && tslint --config '../tslint.json' 'app/**/*.ts' --exclude '**/node_modules/**'"
  }
}
